{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RoomCleanup": {
    "RoomWithoutHostTtlMinutes": 10,
    "DisconnectedPlayerGraceSeconds": 120,
    "CleanupIntervalSeconds": 30,
    "Enabled": true
  },
  "Autoplay": {
    "Enabled": false,
    "MinBots": 4,
    "MaxBots": 12,
    "PollIntervalMs": 250,
    "MinActionDelayMs": 250,
    "MaxActionDelayMs": 1200,
    "DefaultTimeLimitSeconds": 15
  }
}
